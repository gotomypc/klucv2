<opencv_storage>
<A type_id="opencv-ann-params">
  
  <activation_function>SIGMOID_SYM</activation_function>
  
  <!-- alpha -->
  <f_param1>1.0</f_param1>
  
  <!-- beta -->
  <f_param2>1.0</f_param2>
  
  <!-- Don't know what these are doing -->
  <min_val>0.0</min_val>
